grid.rect(
x = unit(0.5, "npc"),
y = unit(0.5, "npc"),
width = unit(1, "npc"),
height = unit(1, "npc"),
gp = gpar(col = NA, fill = "white")
)
for (i in seq(0, 1, by = 0.1)) {
grid.lines(
x = unit(c(i, i), "npc"),
y = unit(c(0, 1), "npc"),
gp = gpar(lty = "dotted", col = "gray80")
)
grid.lines(
x = unit(c(0, 1), "npc"),
y = unit(c(i, i), "npc"),
gp = gpar(lty = "dotted", col = "gray80")
)
}
}
