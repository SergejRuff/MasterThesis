class(col) %>%
gsub("character", "chr", .) %>%
gsub("numeric", "dbl", .) %>%
gsub("integer", "int", .) %>%
gsub("factor", "fct", .) %>%
gsub("Date", "date", .)
}),
Examples = sapply(vg_file, get_examples),
stringsAsFactors = FALSE
)
