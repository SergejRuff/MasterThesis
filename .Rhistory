Examples = "Example Values"
) %>%
fmt_markdown(
columns = c(Examples)
) %>%
cols_align(
align = "left",
columns = c(Column_Name, Data_Type, Examples)
) %>%
fmt_number(
columns = c(Examples),
decimals = 2
)
